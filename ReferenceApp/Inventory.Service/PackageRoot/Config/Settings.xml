<?xml version="1.0" encoding="utf-8" ?>
<Settings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.microsoft.com/2011/01/fabric">
  <!-- This is used by the StateManager's replicator. -->
  <Section Name="ReplicatorConfig">
    <Parameter Name="ReplicatorEndpoint" Value="ReplicatorEndpoint" />
  </Section>
  <Section Name="Inventory.Service.Settings">
    <Parameter Name="BackupMode" Value="local" />
  </Section>
  <Section Name="Inventory.Service.BackupSettings.Azure">
    <Parameter Name="BackupAccountName" Value="TBD=" />
    <Parameter Name="PrimaryKeyForBackupTestAccount" Value="TBD=" />
    <Parameter Name="BlobServiceEndpointAddress" Value="https://TBD.net/" />
    <Parameter Name="BackupFrequencyInMinutes" Value="1" />    
  </Section>
  <Section Name="Inventory.Service.BackupSettings.Local">
    <Parameter Name="LocalBackupTempPath" Value="C:\temp" />
    <Parameter Name="BackupFrequencyInMinutes" Value="1" />
  </Section>

</Settings>
